{
    "name": "Chrome Extension Example",
    "version": "0.1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions": ["storage"],

    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}